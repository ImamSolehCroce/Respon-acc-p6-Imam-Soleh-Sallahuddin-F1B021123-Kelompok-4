package respon.acc.p6;

/**
 *
 * @author Lenovo
 */
class kucing extends Hewan {
    private String makan;

    public kucing(String nama) {
        super(nama);
    }

    public kucing(String nama, String makan) {
        super(nama);
        this.makan = makan;
    }
       @Override
    public void makan() {
        System.out.println(getNama() + " (kucing) makan tulang.");
        System.out.println(getNama() + " (kucing) makan jajan.");
    }

    private String getNama() {
        throw new UnsupportedOperationException("Not supported yet."); //To change body of generated methods, choose Tools | Templates.
    }
}
